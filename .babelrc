{
  "presets": ["next/babel"],
  "plugins": [
    ["styled-components", { "ssr": true }],
    [
      "module-resolver",
      {
        "root": ["./src"],
        "extensions": [".tsx", ".ts"],
        "alias": {
          "@shared": "./src/shared",
          "@scenes": "./src/scenes"
        }
      }
    ]
  ]
}
